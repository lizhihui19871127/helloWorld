#extends("wtgf:page/layout/layoutBind.vm")
    #block("page_var")
    #set($currentNav="account")
    #set($GF_VIEWINFO.mainNav='r5')
    #set($GF_VIEWINFO.subNav='c51')
    #end
    #block("content_main")

    <div class="content-main">
        <!-- 银行卡-START -->
        <div class="container-bank wrap" style="margin-left: 0px;">
            <div class="pageMain" style="padding:0 0 0 0;padding-bottom:2px">
                <div class="bank-main my-bank-main">
                    <div class="my-bank-title">
                        正在跳转至银行网关，请稍后…………
                        <form name="frm" id="frm" action="$!{reqUrl}" method="POST">
                            #foreach($form in $formdata.entrySet())
                                <input type="hidden" name="$form.key" id="$form.key" value="$form.value" />
                            #end
                        </form>

                        <input type="hidden" name="verifyBusiness" id="verifyBusiness" value="${verifyBusiness}"/>
                        <input type="hidden" name="verifyMethod" id="verifyMethod" value="${verifyMethod}"/>
                    </div>
                </div>
            </div>
        </div>
        <!-- 银行卡-END -->
    </div>
    #end
    #require("wtgf:page/bindingCard/goBankUrl/goBankUrl.vm")

#end
