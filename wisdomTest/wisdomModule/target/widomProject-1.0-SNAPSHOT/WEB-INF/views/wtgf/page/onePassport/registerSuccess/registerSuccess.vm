#extends("wtgf:page/layout/newlayoutNoNav.vm")
#block("page_var")
#set($HEADER_TITLE="欢迎进入广发基金网上开户注册系统！")
#set($BOOTSTRAP_CONTAINER="")
#set($IS_LOGIN=0)
#end
#block("header_scripts")
<script type="text/javascript">
    window.WTjson = {
        "WT.si_n":"开户流程",
        "WT.si_x":"填写资料"
    }
</script>
