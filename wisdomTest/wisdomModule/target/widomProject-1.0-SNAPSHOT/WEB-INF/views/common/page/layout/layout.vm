    <script type="text/javascript">
        //expose the template data to the js
        //the data in the $GF_VIEWINFO can't be an Array or Object,only accept number,string,boolean
        window.GF_VIEWINFO = {
            #foreach($key in $GF_VIEWINFO.keySet())
                "$key" : "$GF_VIEWINFO.get($key)",
