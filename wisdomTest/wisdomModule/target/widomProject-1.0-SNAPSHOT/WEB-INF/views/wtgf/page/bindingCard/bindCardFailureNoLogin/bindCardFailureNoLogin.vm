#extends("wtgf:page/layout/layoutNoNav.vm")
#block("page_var")
#set($HEADER_TITLE="欢迎进入广发基金网上开户注册系统！")
#set($BOOTSTRAP_CONTAINER="")
#end

#block("content_main")
    <p class="empty-line"></p>
    <div class="container">
        <div class="row">
            <div class="span8 offset2">
                <p class="gflogo1"></p><p class="gflogo2"></p>
            </div>
        </div>
    </div>
    <p class="line linehead"></p>
    <div class="container">
        <div class="bank-main tsqy-bank-main">
            <div class="pay-main pay-over-main">
                <div class="info-back fail-info-back" style="width: 450px;">
                    <h3 style="padding-left: 30px;margin-bottom: 20px;">操作失败，可能失败原因：$!{returnmsg}</h3>
                    <input type="hidden" name="returnmsg" id="returnmsg" value="$!{returnmsg}" />
                    <input type="hidden" name="verifyBusiness" id="verifyBusiness" value="${verifyBusiness}"/>
                    <input type="hidden" name="verifyMethod" id="verifyMethod" value="${verifyMethod}"/>
                    <br/>
                    <span style="font-size: 14px;padding-left: 35px;">遇到问题请致电我司客服热线：95105828
                    </span>
                </div>
            </div>
        </div>
    </div>
    #end
    #require("wtgf:page/bindingCard/bindCardFailureNoLogin/bindCardFailureNoLogin.vm")

#end
