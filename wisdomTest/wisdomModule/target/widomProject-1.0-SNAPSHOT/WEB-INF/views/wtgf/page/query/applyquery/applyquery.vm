#extends("wtgf:page/layout/layout.vm")
    #block("content_main")
<form name="queryForm" id="queryForm" action="$action" method="post">
<div class="successfix">
    <input type="hidden" id="segDate" name="segDate" value="$segDate" />
<center>
<table height="80px" cellspacing="5" cellpadding = "5" width="100%" style="margin:10px">
<tr style="height: 40px;">
    <td style="width:80px;">期间：</td>
    <td style="width:360px;height: 29px;">
        <div class="bank" id="selDIV" style="display:inline;float:left;">
            <div class="bankshow" id="creditBank" name="creditBank">最近一月</div>
            <div class="banklist" id="banklist" style="display: none" >
                <ul style="padding-left:5px;margin:0px;">
                    <li  bankkey="1" >
                        <a    href="javascript:;">最近一周</a>
                    </li>
                    <li   bankkey="2">
                        <a      href="javascript:;">最近一个月</a>
                    </li>
                    <li   bankkey="3">
                        <a   href="javascript:;">最近三个月</a>
                    </li>
                    <li   bankkey="4">
                        <a     href="javascript:;">最近半年</a>
                    </li>
                    <li   bankkey="5">
                        <a  href="javascript:;">最近一年</a>
                    </li>
                    <li   bankkey="6">
                        <a      href="javascript:;">自定义时间</a>
                    </li>
                </ul>
            </div>
        </div>
        <div id="timeDIV" style="display: none">
        <div class="nowarp" >
            <input type="hidden" name="startDate" id="startDate" value="$startDate"/>
            <input type="text" id="startDatePicker" class="Wdate" readonly="readonly" style="width:135px;cursor:pointer" value="$startDate"/>
             -
            <input type="hidden" name="endDate" id="endDate" value="$endDate"/>
            <input type="hidden" name="endDateVal" id="endDateVal" value="$endDateVal"/>
            <input type="text" id="endDateTextPicker" class="Wdate"  readonly="readonly" style="width:135px;cursor:pointer" value="$endDate"/>
           　　　　　　　　　　　　
       </div>
            <div class="timeselector" >
                <div class="timeshow" ></div>
                <div class="timelist" >
                    <ul style="padding-left:5px;margin:0px;">
                        <li  timekey="1" >
                            <a    href="javascript:;">最近一周</a>
                        </li>
                        <li   timekey="2">
                            <a      href="javascript:;">最近一个月</a>
                        </li>
                        <li   timekey="3">
                            <a   href="javascript:;">最近三个月</a>
                        </li>
                        <li   timekey="4">
                            <a     href="javascript:;">最近半年</a>
                        </li>
                        <li   timekey="5">
                            <a  href="javascript:;">最近一年</a>
                        </li>
                        <li   timekey="6">
                            <a      href="javascript:;">自定义时间</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </td>
    <td style="width:80px">基金名称：</td>
    <td style="text-align: left;">
       <input type="text" name="province" id="province" style="width: 240px;text-align: left;color: #999" AUTOCOMPLETE ="off" onkeyup="searchDev(this.value)" value="请输入基金名称、代码" />
          <input type="hidden" name="fundCode" id="fundCode" value="$fundCode" />
        <input type="hidden" name="fundName" id="fundName" value="$fundName" />

    </td>

<td></td>					   
</tr>

<tr>
    <td>业务类型：</td>
    <td>
        <div class="bussines"  style="display:inline;float:left;">
            <div class="bussshow" id="bussinfo" name="bussinfo">全部</div>
            <div class="busslist" id="busslist" style="display: none" >

            </div>

        </div>
        <input type="hidden" id="businType" name="businType" value="$businType" />


    </td>
<td>销售渠道：</td>
<td>
    <div class="tradeCap"  style="display:inline;float:left;">
        <div class="tradeshow" id="tradeCap" name="tradeCap">全部</div>
        <div class="tradelist" id="tradeCaptal" style="display: none" >

        </div>

    </div>

    <input type="hidden" id="tradeCapital" name="tradeCapital" value="$tradeCapital" />
</td>


<td align="left">
<!--<button type="submit" class="btn btn-success" contenteditable="false">&nbsp;&nbsp;查询&nbsp;&nbsp;</button>-->
<input style="width:90px" name="login_b1" id="J_submitButton" type="button" class="submit" value=" 查 询 "/>
</td>
</tr>
</table>
</center>
</div>
<br />
<div class="tabbable">
<ul class="nav nav-tabs" contenteditable="false" id="myTab">
#if($userType == 0 || $userType == 2)
#if(!$deleReqList)
<li>
#else
<li class="active">
#end
<a href="javascript:void(0)" class="saleRecord">直销渠道交易记录
<i class="icon-question-sign" id="tip1" title="直销渠道交易记录：指在广发基金直销（网上交易、柜台交易）、广发基金APP、钱袋子APP、广发基金官方微信、广发基金淘宝店、百度金融、广发证券淘金钱包的交易记录查询。"></i></a></li>
#end
#if($userType == 1 || $userType == 2)
#if(!$deleReqOtherList)
<li>
#else
<li class="active">
#end
<a href="javascript:void(0)" data-toggle="tooltip" class="otherSaleRecord">其他渠道交易记录
<i class="icon-question-sign" id="tip2" title="其他渠道交易记录：指在银行、券商、第三方销售机构（包括苏宁、理财通）的交易记录查询。" ></i></a></li>
#end
</ul>
<center>

<div class="tab-content">
#if(!$deleReqList)
<div class="tab-pane" id="zxSale">
#else
<div class="tab-pane active" id="zxSale">
#end
		<table class="table table-bordered" contenteditable="false">
		<thead>
		<tr>
		<th>申请日期</th>
		<th>基金名称</th>
		<th>业务类型</th>
		<th>交易份额</th>
		<th>交易金额</th>
		<th>销售渠道</th>
		<th>收费方式</th>
		<th>申请状态</th>
		<th>确认状态</th>
		<th>操作</th>
		</tr>
		</thead>
		<tbody>
		<input type="hidden" name="userType" value="${userType}"/>
		#foreach($deleReqInfo in $deleReqList)
		#if($velocityCount%2 == 0)
		<tr style="background-color: rgb(244, 244, 244);">
		#else
		<tr>
		#end
		<td>$deleReqInfo.requestDate</td>
		<td>$deleReqInfo.fundNameView</td>
		<td>$deleReqInfo.businTypeView</td>
		<td>
		#if($deleReqInfo.requestShare == "0.0")
		--
		#else
		$deleReqInfo.requestShareView份
		#end
		</td>
		<td>
		#if($deleReqInfo.requestBala == "0.0")
		--
		#else
		$deleReqInfo.requestBalaView元
		#end
		</td>
		<td>$deleReqInfo.reqCapitalView</td>
		<td>
			#if($deleReqInfo.businType == '029')
				--
			#else
				$deleReqInfo.shareTypeView
			#end
		</td>
		<td>
		#if($deleReqInfo.stateView == "未校验")
			未校验[<a href="javascript:void(0)" title="扣款状态未知，指的是在点击“申购”后没有完成支付或支付完成后，银行还没将支付信息提交我司。">?</a>]
		#else
			$deleReqInfo.stateView
		#end
		</td>
		<td>$deleReqInfo.confirmView</td>
		<td align="center">
		#if($deleReqInfo.businTypeView != "送货币基金" && $deleReqInfo.businTypeView != "修改分红方式为红利再投" && $deleReqInfo.businTypeView != "修改分红方式为现金分红")
			<a href="javascript:void(0)" class="showDetailInfo">明细</a>
			#set($notempty = '1')
		#end
		#if($deleReqInfo.beCancelled == "true" && $deleReqInfo.businType != "送货币基金")
			<a href="javascript:void(0)" class="rollback">撤单</a>&nbsp;
			#set($notempty = '1')
		#end
        #if(!$notempty && ($deleReqInfo.businTypeView == "修改分红方式为红利再投" || $deleReqInfo.businTypeView == "修改分红方式为现金分红" || $deleReqInfo.businTypeView == "送货币基金") )
               --
        #end
		<input type="hidden" value="$deleReqInfo.delegateNo"/>
		<input type="hidden" value="$deleReqInfo.businTypeView"/>
		<input type="hidden" value="$deleReqInfo.requestType"/>
		<input type="hidden" value="$deleReqInfo.redomDay"/>
		<input type="hidden" value="$deleReqInfo.businType"/>
        <input type="hidden" value="$deleReqInfo.tradeAcco"/>
		</td>
		</tr>
		#end
		</tbody>
		</table>
	</div>
	#if(!$deleReqList)
	<div class="tab-pane active" id="otherSale">
	#else
	<div class="tab-pane" id="otherSale">
	#end
		<table class="table table-bordered" contenteditable="false" id="otherSale" class="tab-pane">
		<thead>
		<tr>
		<th>确认日期</th>
		<th>基金名称</th>
		<th>业务类型</th>
		<th>确认份额</th>
		<th>确认金额</th>
		<th>销售渠道</th>
		<th>收费方式</th>
		<th>手续费</th>
		<th>确认状态</th>
		</tr>
		</thead>
		<tbody>
		#foreach($deleReqInfo in $deleReqOtherList)
		#if($velocityCount%2 == 0)
		<tr style="background-color: rgb(244, 244, 244);">
		#else
		<tr>
		#end
		<td>$deleReqInfo.applyNo</td>
		<td>$deleReqInfo.fundCode</td>
		<td>$deleReqInfo.businCode</td>
		<td>$deleReqInfo.confirmShareView份</td>
		<td>$deleReqInfo.confirmBalanceView元</td>
		<td>#if($deleReqInfo.netNo != "") $deleReqInfo.netNo #else 广发基金 #end</td>
		<td>$deleReqInfo.shareType</td>
		<td>$deleReqInfo.fare元</td>
		<td>$deleReqInfo.confirmFlag</td>
		</tr>
		#end
		</tbody>
		</table>
	</div>
</div>
</div>

<div class="pagination" style="height:48px;line-height:48px">
<ul contenteditable="false" style="line-height:48px;vertical-align:middle;">
<li><a id="home" href="javascript:void(0)">首页</a></li>
<li><a id="before" href="javascript:void(0)">上一页</a></li>
<li><a id="next" href="javascript:void(0)">下一页</a></li>
<li><a id="last" href="javascript:void(0)">末页</a></li>
</ul>
<span style="margin-left:100px;line-height:48px;vertical-align:middle;height:48px">
<input type="hidden" id="lastNo" value="${pageNum}">
总条数：${listCount},总页数：${pageNum}    当前页&nbsp;
    <input type="text" id="pageNo" name="pageNo" value = "${pageNo}" onkeyup="value=value.replace(/[^\d]/g,'')"; style="width:60px;height:14px"> <a href="javascript:queryForm.submit()">跳转</a></span>
<span style="margin-left:100px;line-height:48px;vertical-align:middle;">每页显示&nbsp;&nbsp;
	<input type="text" id="pageSize" name="pageSize" value = "${pageSize}" onkeyup="value=value.replace(/[^\d]/g,'')"; style="width:60px;height:14px"/> <a href="javascript:queryForm.submit()">设置</a>
</span>

</div>
</center>
</form>
<input type="hidden" id="bussinessList" name="bussinessList" value='$bussinessList' />
<input type="hidden" name="fundlist" id="fundlist" value='$fundList' />
<input type="hidden" id="tradeCaptalList" name="tradeCaptalList" value='$saleSourceList' />

<div id="container">
##提示框
<div id="myModal" class="modal hide fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
	<div class="modal-header">
		<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
		<h3 id="myModalLabel">
			<font size="4">交易记录明细</font>
		</h3>
	</div>
	
	<div class="modal-body" style="max-height:1200px" id="bodyHtml">
	
	</div>
	
	<div class="modal-footer">
		<button class="btn" data-dismiss="modal" aria-hidden="true">关闭</button>
	</div>
</div>	
</div>

<div style="width:96%; margin:30px auto 10px auto; height: 35px;background:url(/images/help.gif) no-repeat 0 20px;border-top:1px solid #e5e5e5;padding-top:5px;">
	<p style="margin:10px 5px 0 0;_margin-right:2px; font-size:13px; float:right;"><a
			href="javascript:scrollTo(0,0);" class="a1">回到顶部</a></p>
</div>
<div style="width:96%; margin:0 auto">
	<ul>
		<li style="padding:5px 0px;background:url(../images/point.png) no-repeat 0 6px;padding-left:15px;">
			为什么查不到我的海外型基金交易记录？
		</li>
		答：由于海外型基金交易下单日为海外休市日时，系统会将该笔交易自动顺延至下一交易日，因此查询时请调整您的结束日期。
	</ul>
</div>
</div>
   
    #end
    #require("wtgf:page/query/applyquery/applyquery.vm")    

#end
